<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>vlocity_ins__EffectiveYearValidation</fullName>
    <active>true</active>
    <errorConditionFormula>(VALUE(vlocity_ins__EffectiveYear__c) &lt;= 1970) || (VALUE(vlocity_ins__EffectiveYear__c) &gt;= 2100)</errorConditionFormula>
    <errorDisplayField>vlocity_ins__EffectiveYear__c</errorDisplayField>
    <errorMessage>Effective Year must be a valid year.</errorMessage>
</ValidationRule>
