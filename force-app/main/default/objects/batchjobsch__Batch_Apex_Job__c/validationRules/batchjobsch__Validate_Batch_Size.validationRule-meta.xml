<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>batchjobsch__Validate_Batch_Size</fullName>
    <active>true</active>
    <errorConditionFormula>OR
(
batchjobsch__Batch_Size__c &lt; 1,
batchjobsch__Batch_Size__c &gt; 2000
)</errorConditionFormula>
    <errorDisplayField>batchjobsch__Batch_Size__c</errorDisplayField>
    <errorMessage>Batch size must be between 1 and 2,000.</errorMessage>
</ValidationRule>
