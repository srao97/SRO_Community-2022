<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>vlocity_ins__WaitlistRemovedReasonNotNull</fullName>
    <active>true</active>
    <errorConditionFormula>IF( vlocity_ins__IsRemoved__c == true &amp;&amp; ISBLANK( vlocity_ins__ReasonForRemoval__c ),  true,  false)</errorConditionFormula>
    <errorDisplayField>vlocity_ins__ReasonForRemoval__c</errorDisplayField>
    <errorMessage>Please select reason/s for removal</errorMessage>
</ValidationRule>
